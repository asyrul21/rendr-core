{
    "root": "public/",
    "headers": {
      "/**": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "/**.png": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/src/images/**.png": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/src/images/**.jpg": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/src/images/**.webp": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/src/images/**.jp2": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/src/data/**.json": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/icons/*.png": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "https_only": true,
    "error_page": "404.html"
  }